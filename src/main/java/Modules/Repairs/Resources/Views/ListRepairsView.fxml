<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="385.0" prefWidth="571.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Modules.Repairs.Controllers.ListRepairsController">
    <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#refresh" text="Refresh" />
                  <Button mnemonicParsing="false" onAction="#add" text="Add" />
               </children>
            </HBox>
              <TableView id="tableRepairs" fx:id="tableRepairs" focusTraversable="false" VBox.vgrow="ALWAYS">
                  <columns>
                      <TableColumn id="id" fx:id="id" editable="false" maxWidth="50.0" prefWidth="30.0" text="Id" />
                  <TableColumn id="dateStart" fx:id="dateStart" editable="false" maxWidth="150.0" prefWidth="100.0" text="dateStart" />
                  <TableColumn id="dateEnd" fx:id="dateEnd" editable="false" maxWidth="150.0" minWidth="0.0" prefWidth="100.0" text="dateEnd" />
                      <TableColumn id="note" fx:id="note" editable="false" prefWidth="110.0" text="Note" />
                      <TableColumn id="carId" fx:id="carId" editable="false" maxWidth="50.0" prefWidth="30.0" text="Car id" visible="false" />
                      <TableColumn id="carRegistrationNumber" fx:id="carRegistrationNumber" editable="false" maxWidth="150.0" prefWidth="130.0" text="Car registration number" />
                      <TableColumn id="createdAt" fx:id="createdAt" editable="false" prefWidth="110.0" text="Created at" />
                      <TableColumn id="updatedAt" fx:id="updatedAt" editable="false" prefWidth="110.0" text="Updated at" />
                  </columns>
              </TableView>
         </children>
      </VBox>
    </children>
</AnchorPane>
